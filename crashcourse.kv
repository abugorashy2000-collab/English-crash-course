     id:show1
     BoxLayout:
          padding: 50
          spacing: 10
          ScrollView:
               id: sv
               do_scroll_y: True
               do_scroll_x: False
               bar_width: '30dp'
               scroll_type: ['bars']
               TextInput:
                    id:punc1
                    text: 'choose lesson'
                    size_hint: None, None
                    size: sv.width - sv.bar_width, dp(450)
                    height:390
                    width:3000
                    
                    multiline: True
                    foreground_color:(0.3,0.4,0.5,1)
                    font_name:"data/arial.ttf"
     BoxLayout:  
          Button:
               size_hint:(0.1,0.1)
               color:(1,1,1,1)
               background_color:(0,1,0,1)
               text:"Go Back"
               on_press:
                    app.root.current="punc1"


<Translation>:
     name:"trans"
     id:t
     BoxLayout:
          padding: 50
          spacing: 10
          ScrollView:
               id: sv
               do_scroll_y: True
               do_scroll_x: False
               bar_width: '30dp'
               scroll_type: ['bars']
               TextInput:
                    id:my
                    text: 'choose lesson'
                    size_hint: None, None
                    size: sv.width - sv.bar_width, dp(450)
                    multiline: True
                    foreground_color:(1,0,0,1)
                    font_name:"data/arial.ttf"
     BoxLayout:
          Button:
               
               text:"Go Back"
               size_hint:(0.1,0.1)
               color:(1,1,1,1)
               background_color:(0,1,0,1)
               on_press:
                    root.checkwindow()

       




